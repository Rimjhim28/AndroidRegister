<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codingblocks.attendancetracker">

    <application xmlns:tools="http://schemas.android.com/tools"
        android:name=".AndroidRegisterApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListOfAbsentPresentStudentsActivity" />
        <activity android:name=".CreateNewCourse" />
        <activity android:name=".AttendanceDatabaseActivity"></activity>
    </application>

</manifest>